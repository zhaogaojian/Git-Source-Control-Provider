    </ItemGroup>
    <PropertyGroup>
      <_TmpVsixDir>%(_TmpVsixPathItem.RootDir)%(_TmpVsixPathItem.Directory)</_TmpVsixDir>
    </PropertyGroup>
    <RemoveDir Directories="$(_TmpVsixDir)" />
